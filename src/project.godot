[application]

config/name="Trashy Towers"
config/description="A Tricky Towers inspired game where you have to stack trash with your roommates until someone spills it."
run/main_scene="res://screens/title_screen/TitleScreen.tscn"
config/icon="res://icon.png"

[autoload]

Settings="*res://Settings.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

